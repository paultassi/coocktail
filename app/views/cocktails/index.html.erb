<h1>Find here our awesome cocktails</h1>
<div>
<ul class='list'>
  <% @cocktails.each do |cocktail| %>
    <li ><%= cocktail.name %> <%#= link_to '(delete)', cocktail_path(@cocktail), method: :delete %>
  <% end %>
</li>
</ul>
</div>

<%= link_to 'add a new cocktail', new_cocktail_path, {class: "btn btn-primary btn-lg"} %>
